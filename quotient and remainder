// 6.Write a program to compute quotient and Remainder based on dividend and divisor.
// Code: 
package boot;
public class QuotientAndRemainder
 {
	public static void main (String [] args)
 {
	int dividend =Integer.parseInt(args [0]); //Taking command line argument and parsing string value to integer
	int divisor =Integer.parseInt(args [1]) ;//Taking command line argument and parsing string value to integer
	int remainder = dividend% divisor;//calculate reminder
	int quotient = (int)dividend/divisor;//calculate quotient
	System.out.println("Remainder is "+remainder);
	System.out.println("Quotient is "+quotient);
	}
}
