//2. Read 5 integers and print the sum of all positive integers
package boot;
// using java.util.scanner package to accept input from keyboard
import java.util.Scanner;
public class Sum {
public static void main (String [] args) 
{
// Initializing The Variables 
int i=0;
int sum=0;
int a;
 // using for loop to enter inputs
for (i=1; i<=5; i++)
  {
// using scanner to accept input from 
Scanner sc=new Scanner(System.in);
System.out.println("enter integers");   
a=sc.nextInt();
//System.out.println(a);
if(a>0)
{
Sum+=a;
System.out.println(sum);
} else if(a<0)
{
System.out.println(0);
} else
{
System.out.println("zero");
}
}
}
}
